              var index = 0;
              $('.news-detail, .article-content').not('.no-galleries').find('p, div').each(function(ip,p){
                  if ($(p).text().replace(/\s+/g, '').length>0) index++;
                  $(p).find('img').each(function(i,e){
                      if (!$(e).parent().is('a')) {
                          $(e).wrap($('<a></a>')
                              .attr('href', $(e).attr('src'))
                              .attr('rel', 'gallery'+index)
                              .attr('class', 'gallery')
                              .attr('title', $(e).attr('title'))
                          );
                      } else {
                          $(e).parent().attr('rel', 'gallery'+index);
                      }
                  });
              });
              $('.news-detail a.gallery, .article-content a.gallery').fancybox();

              var offset = $(window).outerHeight()*1;
              var duration = 500;
              jQuery(window).scroll(function() {
                  if (jQuery(this).scrollTop() > offset) {
                      jQuery('.back-to-top').fadeIn(duration);
                  } else {
                      jQuery('.back-to-top').fadeOut(duration);
                  }
              });

              jQuery('.back-to-top').click(function(event) {
                  event.preventDefault();
                  jQuery('html, body').animate({scrollTop: 0}, duration);
                  return false;
              })

          });
