{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "campo/random-user-agent": "^1.3",
        "doctrine/dbal": "^3.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.2",
        "ixudra/curl": "^6.20",
        "laravel/framework": "^9.19",
        "laravel/helpers": "^1.3",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "livewire/livewire": "^2.4",
        "ramsey/uuid": "^4.1",
        "spatie/data-transfer-object": "^3.9.1",
        "spatie/enum": "^3.13",
        "spatie/image": "^2.2.4",
        "spatie/laravel-blade-javascript": "^2.7",
        "spatie/laravel-medialibrary": "^10.5.2",
        "symfony/css-selector": "^5.1",
        "symfony/dom-crawler": "^5.1",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fakerphp/faker": "^1.18",
        "friendsofphp/php-cs-fixer": "^3.5",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5.10",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-ignition": "^1.5",
        "spatie/laravel-web-tinker": "^1.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-medialibrary"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
        "ziggy-routes": "php artisan ziggy:generate ./resources/js/helpers/ziggy.js"
    }
}
